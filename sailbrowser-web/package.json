{
  "name": "sailbrowser-web",
  "version": "0.0.0",
  "scripts": {
    "serve": "ng serve",
    "build": "ng build",
    "stats": "ng build --stats-json --named-chunks && npx @angular-experts/hawkeye dist/splitsbrowser/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.2",
  "dependencies": {
    "@angular/cdk": "~21.1.2",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/fire": "^20.0.0",
    "@angular/forms": "^21.1.0",
    "@angular/material": "~21.1.2",
    "@angular/material-date-fns-adapter": "^21.0.5",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@ngbracket/ngx-layout": "15.1.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/material": "^21.1.2",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8",
    "firebase-tools": "^14.0.0"
  },
  "overrides": {
    "@angular/fire": {
      "@angular/common": "$@angular/common",
      "@angular/core": "$@angular/core",
      "@angular/platform-browser": "$@angular/platform-browser",
      "@angular/router": "$@angular/router",
      "@angular/platform-server": "21.0.6",
      "@angular/platform-browser-dynamic": "21.1.2"
    }
  }
}